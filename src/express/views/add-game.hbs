<form method="POST" autocomplete="off" >
  <table
    class="textCenter textBig"
    style="width: 80%; margin: auto;"
  >
    {{#each inputs}}
    <tr>
      <td
      >
        <input
          type="number"
          name="inputs[{{@index}}][ranking]"
          value="{{ranking}}"
          min="1"
          max="4"
          tabindex="-1"
        />
      </td>
      <td
      >
        <input
          name="inputs[{{@index}}][player]"
          placeholder="PLAYER NAME"
          value="{{player}}"
          list="playerList"
        />
        <datalist id="playerList">
          {{#each ../players}}
          <option value="{{this}}" />
          {{/each}}
        </datalist>
      </td>
    </tr>
    {{/each}}
    <tr>
      <td>
      </td>
      <td
        style="padding: 32px 0px"
      >
        <input
          type="password"
          name="password"
          placeholder="PASSWORD"
          autocomplete="new-password"
        />
      </td>
    </tr>
  </table>
  <br/>

  <div class="textCenter">
    <button type="submit">
      SAVE
    </button>
  </div>
</form>