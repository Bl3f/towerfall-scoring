<form method="POST" autocomplete="off" >
  <table class="textCenter textBig">
    {{#each inputs}}
    <tr>
      <td>
        <input type="number" name="inputs[{{@index}}][ranking]" value="{{ranking}}" min="1" max="4" tabindex="-1" />
      </td>
      <td>
        <input type="textfield" name="inputs[{{@index}}][player]" placeholder="PLAYER NAME" value="{{player}}" />
      </td>
    </tr>
    {{/each}}
    <tr>
      <td>
      </td>
      <td>
        <input type="password" name="password" placeholder="PASSWORD" autocomplete="new-password" />
      </td>
    </tr>

  </table>
  <br/>

  <div class="textCenter">
    <button type="submit">
      SAVE
    </button>
  </div>
</form>